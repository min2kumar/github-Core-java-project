create table message
( 
sender  varchar(30),
name varchar(20),
msg varchar(250),
date varchar(40)
)

drop table book


create table book
(
 id int primary key,
name varchar(35),
  author varchar(30),
  btype varchar(32),
   price int,

  link varchar(max),
     unique(name)
)


drop table tempblock

create table tempblock
(
uname varchar(30) primary key
)

select * from book

delete from message

create table maintain
(
mode varchar(10) primary key
)

