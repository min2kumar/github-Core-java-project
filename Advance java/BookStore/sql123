create table data(
id varchar(10) primary key,
uname varchar(30),
name varchar(30),
email varchar(30),
csname varchar(30),
mobile varchar(12),
pwd varchar(25),
unique(uname),
unique(mobile)
)




create table data(
id varchar(10) primary key,
uname varchar(30),
name varchar(30),
email varchar(30),
csname varchar(30),
mobile varchar(12),
pwd varchar(25),
unique(uname),
unique(mobile)
)











drop table data;

select * from data
insert into data values(1,'mintu','mintuc64@gmail.com','tolani college of commerce','8655387871','shailadevi')
